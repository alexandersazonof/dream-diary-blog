<div class="card my-4">
  <h5 class="card-header">Search</h5>
  <div class="card-body">
    <%= form_tag(search_path, :method => "get") do %>
    <div class="input-group">
      <%= search_field_tag :search, params[:search], placeholder: "Search for...", class: "form-control" %>
      <span class="input-group-btn">
        <%= button_tag "Go!", :class => 'btn btn-secondary',:name => nil%>

      </span>
    </div>
    <% end %>
  </div>
</div>
